@model ErasmusPlus.Models.ViewModels.Admin.UniversityAgreementsViewModel
@{
    ViewBag.Title = "University agreements";
}

<div class="row">
    <div class="box">
        <div class="box-header with-border">
            <h3> University agreements </h3>
        </div>
        <div class="box-body">
            <table class="table table-bordered">
                <thead>
                    <tr>
                        <th>Id</th>
                        <th>Source university</th>
                        <th>Target university</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model.UniversityAgreements)
                    {
                        <tr>
                            <td>@item.Id</td>
                            <td>@item.SourceUniversity</td>
                            <td>@item.TargetUniversity</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>