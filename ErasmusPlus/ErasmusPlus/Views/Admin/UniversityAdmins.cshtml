@model List<ErasmusPlus.Models.ViewModels.Admin.UserViewModel>
@{
    ViewBag.Title = "University admins";
}
<div class="row">
    <div class="box">
        <div class="box-header with-border">
            <h3> University admins </h3>
        </div>
        <div class="box-body">
            <table class="table table-bordered">
                <thead>
                    <tr>
                        <th>Personal code</th>
                        <th>First name</th>
                        <th>Last name</th>
                        <th>Username</th>
                        <th>Email</th>
                        <th>Actions</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>@item.PersonalCode</td>
                            <td>@item.FirstName</td>
                            <td>@item.LastName</td>
                            <td>@item.Username</td>
                            <td>@item.Email</td>
                            <td>
                                @Html.ActionLink("Edit", "EditUniversityAdmin", "Admin", new {userId = item.UserId}, new {@class = "btn btn-primary"})
                                @Html.ActionLink("Delete", "EditUniversityAdmin", "Admin", new {userId = item.UserId}, new {@class = "btn btn-warning"})
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>